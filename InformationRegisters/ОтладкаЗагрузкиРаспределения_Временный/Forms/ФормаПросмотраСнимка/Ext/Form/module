
Процедура ПередОткрытием(Отказ, СтандартнаяОбработка)
	
	Набор = РегистрыСведений.ОтладкаЗагрузкиРаспределения_Временный.СоздатьНаборЗаписей();
	Набор.Отбор.Дата.Установить(Дата);
	Набор.Отбор.Пользователь.Установить(Пользователь);
	Набор.Прочитать();
	Если Набор.Количество() > 0 Тогда
		Снимок = Набор[0].ТабСнимок.Получить();
		Если ТипЗнч(Снимок) = Тип("ТаблицаЗначений") Тогда
			ТабличноеПоле1 = Снимок;
			ЭлементыФормы.ТабличноеПоле1.СоздатьКолонки();
		КонецЕсли;	
	КонецЕсли;	
	
КонецПроцедуры
