
Процедура РегистрСведенийСписокПередУдалением(Элемент, Отказ)
	
	Отказ = Истина;
	
КонецПроцедуры

Процедура РегистрСведенийСписокВыбор(Элемент, ВыбраннаяСтрока, Колонка, СтандартнаяОбработка)
	
	СтандартнаяОбработка = Ложь;
	
КонецПроцедуры

Процедура РегистрСведенийСписокПередНачаломИзменения(Элемент, Отказ)
	
	Отказ = Истина;
	
КонецПроцедуры

Процедура ПриОткрытии()
	
	ФормаОбработки = Обработки.УстановкаЛимитовАренднойПлаты.ПолучитьФорму();
	
	Для Каждого ЭлементОтбора Из Отбор Цикл
		Если ЭлементОтбора.Использование Тогда
			НайденныйЭлемент = ФормаОбработки.ЭлементыФормы.ТаблицаЛимитов.ОтборСтрок.Найти(ЭлементОтбора.Имя);
			Если НайденныйЭлемент<>Неопределено Тогда
				ЗаполнитьЗначенияСвойств(НайденныйЭлемент, ЭлементОтбора);
			КонецЕсли; 
		КонецЕсли; 
	КонецЦикла;  
	
	ФормаОбработки.Открыть();
	
	Закрыть();
	
КонецПроцедуры
