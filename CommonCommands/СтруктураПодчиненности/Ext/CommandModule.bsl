
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)
	
	Если ЗначениеЗаполнено(ПараметрКоманды) Тогда
		//+++АК BELN 2018.06.25 
		Если ТипЗнч(ПараметрКоманды)=Тип("ДокументСсылка.РасходныйОрдерСклад") Тогда
			ВидОперации=ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ПараметрКоманды,"ВидОперации");
			Если ВидОперации=ПредопределенноеЗначение("Перечисление.ВидыОперацийРасходСкладскойУчет.ОтгрузкаВТорговуюТочку") Тогда
				ОткрытьФорму("ОбщаяФорма.СтруктураПодчиненностиРО",Новый Структура("ОбъектОтбора", ПараметрКоманды),
				ПараметрыВыполненияКоманды.Источник,
				ПараметрыВыполненияКоманды.Источник.КлючУникальности,
				ПараметрыВыполненияКоманды.Окно);
	      	Иначе
				ОткрытьФорму("ОбщаяФорма.СтруктураПодчиненности",Новый Структура("ОбъектОтбора", ПараметрКоманды),
				ПараметрыВыполненияКоманды.Источник,
				ПараметрыВыполненияКоманды.Источник.КлючУникальности,
				ПараметрыВыполненияКоманды.Окно);
			КонецЕсли; 
		
		Иначе	
			ОткрытьФорму("ОбщаяФорма.СтруктураПодчиненности",Новый Структура("ОбъектОтбора", ПараметрКоманды),
			ПараметрыВыполненияКоманды.Источник,
			ПараметрыВыполненияКоманды.Источник.КлючУникальности,
			ПараметрыВыполненияКоманды.Окно);
		
		
		КонецЕсли; 

		//---АК BELN 2018.06.25 

	КонецЕсли;

КонецПроцедуры

