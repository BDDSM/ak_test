
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)
	Параметры=ОткрытьФормуМодально("Документ.ПриходныйОрдерСклад.Форма.ФормаВыбораРеквизитовТОРГ13");
	Если Параметры=Неопределено Тогда
		Возврат ;
	КонецЕсли;	
	ТабДок = Печать(ПараметрКоманды,Параметры);
	Если ТабДок = Неопределено Тогда
		Возврат;
	КонецЕсли;
	
	ТабДок.ОтображатьСетку 		= Ложь;
	ТабДок.ТолькоПросмотр 		= Истина;
	ТабДок.ОтображатьЗаголовки 	= Ложь;
	ТабДок.Показать();
	
	Если Параметры.ПечататьВетСвидетельства Тогда
		ТабВетСвидетельства = Документы.ПриходныйОрдерСклад.ПолучитьТабДокВетСвидетельства(ПараметрКоманды,Параметры);
		Если ТабВетСвидетельства <> Неопределено Тогда
			ТабВетСвидетельства.ОтображатьСетку 		= Ложь;
			ТабВетСвидетельства.ТолькоПросмотр 		= Истина;
			ТабВетСвидетельства.ОтображатьЗаголовки 	= Ложь;
			ТабВетСвидетельства.Показать("Вет. свидетельства");
		КонецЕсли;	
	КонецЕсли;	
КонецПроцедуры


&НаСервере
Функция Печать(ПараметрКоманды,Параметры)
	
	Возврат Документы.ПриходныйОрдерСклад.ПечатьТОРГ13(ПараметрКоманды,Параметры);
	
КонецФункции

