		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаданиеНаРазборкуТовары.Ссылка.Склад,
	ЗаданиеНаРазборкуТовары.Ссылка.Номенклатура,
	ЗаданиеНаРазборкуТовары.Ссылка.Характеристика,
	СУММА(ЗаданиеНаРазборкуТовары.Количество) КАК Количество,
	ЗаданиеНаРазборкуТовары.ДатаПроизводства
ПОМЕСТИТЬ втНом
ИЗ
	Документ.ЗаданиеНаРазборку.Товары КАК ЗаданиеНаРазборкуТовары
ГДЕ
	ЗаданиеНаРазборкуТовары.Ссылка.Проведен
	И ЗаданиеНаРазборкуТовары.Ссылка.Дата МЕЖДУ ДОБАВИТЬКДАТЕ(&amp;НачалоПериода, ДЕНЬ, 1) И ДОБАВИТЬКДАТЕ(&amp;КонецПериода, ДЕНЬ, 1)
	И ЗаданиеНаРазборкуТовары.Ссылка.Склад.Владелец = &amp;Склад

СГРУППИРОВАТЬ ПО
	ЗаданиеНаРазборкуТовары.Ссылка.Склад,
	ЗаданиеНаРазборкуТовары.Ссылка.Номенклатура,
	ЗаданиеНаРазборкуТовары.Ссылка.Характеристика,
	ЗаданиеНаРазборкуТовары.ДатаПроизводства
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТоварыНаСкладахОстатки.Склад,
	ТоварыНаСкладахОстатки.Номенклатура,
	ТоварыНаСкладахОстатки.Характеристика,
	СУММА(ТоварыНаСкладахОстатки.КоличествоОстаток) КАК КоличествоОстаток,
	ТоварыНаСкладахОстатки.ДатаПроизводства
ПОМЕСТИТЬ втНачОст
ИЗ
	РегистрНакопления.ТоварыНаСкладах.Остатки(
			&amp;НачалоПериода,
			(Номенклатура, Склад, Характеристика) В
				(ВЫБРАТЬ
					втНом.Номенклатура,
					втНом.Склад,
					втНом.Характеристика
				ИЗ
					втНом)) КАК ТоварыНаСкладахОстатки

СГРУППИРОВАТЬ ПО
	ТоварыНаСкладахОстатки.Склад,
	ТоварыНаСкладахОстатки.Номенклатура,
	ТоварыНаСкладахОстатки.Характеристика,
	ТоварыНаСкладахОстатки.ДатаПроизводства
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТоварыНаСкладахОбороты.Склад,
	ТоварыНаСкладахОбороты.Номенклатура,
	ТоварыНаСкладахОбороты.Характеристика,
	СУММА(ТоварыНаСкладахОбороты.КоличествоПриход) КАК КоличествоПриход,
	ТоварыНаСкладахОбороты.ДатаПроизводства
ПОМЕСТИТЬ втДвиж
ИЗ
	РегистрНакопления.ТоварыНаСкладах.Обороты(
			ДОБАВИТЬКДАТЕ(&amp;НачалоПериода, СЕКУНДА, 1),
			&amp;КонецПериода,
			,
			(Номенклатура, Склад, Характеристика) В
				(ВЫБРАТЬ
					втНом.Номенклатура,
					втНом.Склад,
					втНом.Характеристика
				ИЗ
					втНом)) КАК ТоварыНаСкладахОбороты

СГРУППИРОВАТЬ ПО
	ТоварыНаСкладахОбороты.Склад,
	ТоварыНаСкладахОбороты.Номенклатура,
	ТоварыНаСкладахОбороты.Характеристика,
	ТоварыНаСкладахОбороты.ДатаПроизводства
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ЕСТЬNULL(втИтог.Номенклатура, втНачОст.Номенклатура), втДвиж.Номенклатура) КАК Номенклатура,
	ЕСТЬNULL(ЕСТЬNULL(втИтог.Характеристика, втНачОст.Характеристика), втДвиж.Характеристика) КАК Характеристика,
	ЕСТЬNULL(ЕСТЬNULL(втИтог.ДатаПроизводства, втНачОст.ДатаПроизводства), втДвиж.ДатаПроизводства) КАК ДатаПроизводства,
	ЕСТЬNULL(втДвиж.КоличествоПриход, 0) КАК КоличествоПриход,
	ЕСТЬNULL(втНачОст.КоличествоОстаток, 0) КАК НачальныйОстаток,
	ЕСТЬNULL(ЕСТЬNULL(втИтог.Склад, втНачОст.Склад), втДвиж.Склад) КАК Склад,
	ЕСТЬNULL(втИтог.Количество, 0) КАК КоличествоРазобрано,
	ЕСТЬNULL(втНачОст.КоличествоОстаток, 0) + ЕСТЬNULL(втДвиж.КоличествоПриход, 0) - втИтог.Количество КАК КоличествоОстаток
ПОМЕСТИТЬ втИтог
ИЗ
	втНом КАК втИтог
		ПОЛНОЕ СОЕДИНЕНИЕ втНачОст КАК втНачОст
		ПО втИтог.Номенклатура = втНачОст.Номенклатура
			И втИтог.Характеристика = втНачОст.Характеристика
			И втИтог.ДатаПроизводства = втНачОст.ДатаПроизводства
			И втИтог.Склад = втНачОст.Склад
		ПОЛНОЕ СОЕДИНЕНИЕ втДвиж КАК втДвиж
		ПО втИтог.Номенклатура = втДвиж.Номенклатура
			И втИтог.Характеристика = втДвиж.Характеристика
			И втИтог.ДатаПроизводства = втДвиж.ДатаПроизводства
			И втИтог.Склад = втДвиж.Склад
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втИтог.Номенклатура,
	втИтог.Характеристика,
	втИтог.ДатаПроизводства,
	втИтог.КоличествоПриход,
	втИтог.НачальныйОстаток,
	втИтог.Склад,
	втИтог.КоличествоРазобрано,
	втИтог.КоличествоОстаток,
	ВЫБОР
		КОГДА втИтог.КоличествоПриход ЕСТЬ NULL
				ИЛИ втИтог.КоличествоПриход = 0
			ТОГДА втИтог.КоличествоПриход
		ИНАЧЕ втИтог.КоличествоПриход / (ВЫРАЗИТЬ(ЗначенияСвойствОбъектов.Значение КАК ЧИСЛО(15, 3)))
	КОНЕЦ КАК КоличествоПриходКор,
	ВЫБОР
		КОГДА втИтог.НачальныйОстаток ЕСТЬ NULL
				ИЛИ втИтог.НачальныйОстаток = 0
			ТОГДА втИтог.НачальныйОстаток
		ИНАЧЕ втИтог.НачальныйОстаток / (ВЫРАЗИТЬ(ЗначенияСвойствОбъектов.Значение КАК ЧИСЛО(15, 3)))
	КОНЕЦ КАК НачальныйОстатокКор,
	ВЫБОР
		КОГДА втИтог.КоличествоРазобрано ЕСТЬ NULL
				ИЛИ втИтог.КоличествоРазобрано = 0
			ТОГДА втИтог.КоличествоРазобрано
		ИНАЧЕ втИтог.КоличествоРазобрано / (ВЫРАЗИТЬ(ЗначенияСвойствОбъектов.Значение КАК ЧИСЛО(15, 3)))
	КОНЕЦ КАК КоличествоРазобраноКор,
	ВЫБОР
		КОГДА втИтог.КоличествоОстаток ЕСТЬ NULL
				ИЛИ втИтог.КоличествоОстаток = 0
			ТОГДА втИтог.КоличествоОстаток
		ИНАЧЕ втИтог.КоличествоОстаток / (ВЫРАЗИТЬ(ЗначенияСвойствОбъектов.Значение КАК ЧИСЛО(15, 3)))
	КОНЕЦ КАК КоличествоОстатокКор
ИЗ
	втИтог КАК втИтог
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
		ПО втИтог.Характеристика = ЗначенияСвойствОбъектов.Объект
			И (ЗначенияСвойствОбъектов.Свойство = ЗНАЧЕНИЕ(ПланВидовХарактеристик.СвойстваОбъектов.КоличествоВУпаковке))</query>
	</dataSet>
	<totalField>
		<dataPath>КоличествоОстаток</dataPath>
		<expression>Сумма(КоличествоОстаток)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоПриход</dataPath>
		<expression>Сумма(КоличествоПриход)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоРазобрано</dataPath>
		<expression>Сумма(КоличествоРазобрано)</expression>
	</totalField>
	<totalField>
		<dataPath>НачальныйОстаток</dataPath>
		<expression>Сумма(НачальныйОстаток)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоОстатокКор</dataPath>
		<expression>Сумма(КоличествоОстатокКор)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоПриходКор</dataPath>
		<expression>Сумма(КоличествоПриходКор)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоРазобраноКор</dataPath>
		<expression>Сумма(КоличествоРазобраноКор)</expression>
	</totalField>
	<totalField>
		<dataPath>НачальныйОстатокКор</dataPath>
		<expression>Сумма(НачальныйОстатокКор)</expression>
	</totalField>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Дата</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>Date</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>false</useRestriction>
		<use>Always</use>
	</parameter>
	<parameter>
		<name>КонецПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Конец периода</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>Date</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>КонецПериода(&amp;НачалоПериода,"День")</expression>
		<use>Always</use>
	</parameter>
	<parameter>
		<name>Период</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Период</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>false</useRestriction>
	</parameter>
	<parameter>
		<name>Склад</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Склад</v8:content>
			</v8:item>
