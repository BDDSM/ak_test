					<dcsset:left xsi:type="dcscor:Field">ТорговаяТочка</dcsset:left>
					<dcsset:comparisonType>Equal</dcsset:comparisonType>
					<dcsset:userSettingID>21e59ef5-88b6-4d82-ace9-bcff889ff947</dcsset:userSettingID>
				</dcsset:item>
			</dcsset:filter>
