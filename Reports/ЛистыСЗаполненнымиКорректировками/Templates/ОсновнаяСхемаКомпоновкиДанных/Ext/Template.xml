						<v8:startDate>0001-01-01T00:00:00</v8:startDate>
						<v8:endDate>0001-01-01T00:00:00</v8:endDate>
					</dcscor:value>
					<dcsset:userSettingID>1a45d5d5-8758-4c20-883b-a2a278f962d7</dcsset:userSettingID>
