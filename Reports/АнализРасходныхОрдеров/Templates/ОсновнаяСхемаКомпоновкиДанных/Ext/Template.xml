////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РасходныйОрдерСкладТовары.Ссылка КАК Документ,
	СУММА(ЕСТЬNULL(ТЗ_Поступило.Получено, 0)) КАК ПолученоТТ,
	СУММА(РасходныйОрдерСкладТовары.КоличествоУРЗ) КАК КоличествоУРЗ
ПОМЕСТИТЬ ТЗ_Итог
ИЗ
	Документ.РасходныйОрдерСклад.Товары КАК РасходныйОрдерСкладТовары
		ЛЕВОЕ СОЕДИНЕНИЕ ТЗ_Поступило КАК ТЗ_Поступило
		ПО РасходныйОрдерСкладТовары.Номенклатура = ТЗ_Поступило.Номенклатура
			И РасходныйОрдерСкладТовары.Ссылка.Получатель = ТЗ_Поступило.ТорговаяТочка
			И (НАЧАЛОПЕРИОДА(ВЫБОР
					КОГДА РасходныйОрдерСкладТовары.Ссылка.ДатаРаспределения &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА РасходныйОрдерСкладТовары.Ссылка.ДатаРаспределения
					ИНАЧЕ РасходныйОрдерСкладТовары.Ссылка.Дата
				КОНЕЦ, ДЕНЬ) = ТЗ_Поступило.День)
ГДЕ
	РасходныйОрдерСкладТовары.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И РасходныйОрдерСкладТовары.Ссылка.ПровереноОператором
	И РасходныйОрдерСкладТовары.Ссылка.ВидОперации = &amp;ВидОперации
	И РасходныйОрдерСкладТовары.Ссылка.Статус = &amp;Статус

СГРУППИРОВАТЬ ПО
	РасходныйОрдерСкладТовары.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТЗ_Итог.Документ,
	ТЗ_Итог.ПолученоТТ,
	ТЗ_Итог.КоличествоУРЗ
ПОМЕСТИТЬ ТЗ_ИтогСвернутый
ИЗ
	ТЗ_Итог КАК ТЗ_Итог
ГДЕ
	(ТЗ_Итог.ПолученоТТ = 0
			ИЛИ ТЗ_Итог.КоличествоУРЗ = 0)
;

