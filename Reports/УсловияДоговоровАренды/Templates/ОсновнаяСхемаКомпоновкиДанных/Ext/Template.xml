			<dataPath>УчитыватьЗалог</dataPath>
			<field>УчитыватьЗалог</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ФормаОплаты</dataPath>
			<field>ФормаОплаты</field>
