		<query>ВЫБРАТЬ
	0 КАК ИД,
	Таблица.Период,
	МЕСЯЦ(Таблица.Период) КАК Месяц,
	ГОД(Таблица.Период) КАК Год,
	Таблица.Счет,
	Таблица.Субконто1 КАК Объект,
	ЕСТЬNULL(ВводВЭксплуатациюОборудование.Местоположение, ТаблицаСостояниеОС.Местоположение) КАК ТорговаяТочка,
	ЕСТЬNULL(ВводВЭксплуатациюОборудование.Местоположение.id_TT, ТаблицаСостояниеОС.Местоположение.id_TT) КАК id_TT,
	Таблица.Счет.Наименование КАК СтатьяДР,
	"" КАК СтатьяДР_Код,
	Таблица.СуммаОборотДт КАК Сумма,
	Таблица.КорСчет
ПОМЕСТИТЬ ТЗ
ИЗ
	РегистрБухгалтерии.Финансовый.Обороты(&amp;НачалоПериода, &amp;КонецПериода, Месяц, Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Инвестиции)), , Организация.Код &lt;&gt; "000000005", , ) КАК Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ВводВЭксплуатацию.Оборудование КАК ВводВЭксплуатациюОборудование
		ПО Таблица.Субконто1 = ВводВЭксплуатациюОборудование.ОсновноеСредство
			И (ВводВЭксплуатациюОборудование.Ссылка.Проведен)
			И (ЛОЖЬ)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СостояниеОС.СрезПоследних(&amp;КонецПериода {(&amp;КонецПериода)}, ) КАК ТаблицаСостояниеОС
		ПО Таблица.Субконто1 = ТаблицаСостояниеОС.ОсновноеСредство
ГДЕ
	НЕ(Таблица.Счет.Код = "01.9"
				И Таблица.КорСчет.Код = "01.1")

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	0,
	Таблица.Период,
	МЕСЯЦ(Таблица.Период),
	ГОД(Таблица.Период),
	Таблица.Счет,
	Таблица.Субконто2,
	Таблица.Субконто1,
	ВЫРАЗИТЬ(Таблица.Субконто1 КАК Справочник.СтруктурныеЕдиницы).id_TT,
	Таблица.Субконто2,
	ВЫРАЗИТЬ(Таблица.Субконто2 КАК Справочник.СтатьиДоходовРасходов).Код,
	Таблица.СуммаОборот,
	Таблица.КорСчет
ИЗ
	РегистрБухгалтерии.Финансовый.Обороты(
			&amp;НачалоПериода,
			ДАТАВРЕМЯ(2017, 4, 30, 23, 59, 59) {(&amp;КП)},
			Месяц,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Финансовый.ЗатратыДляРаспределения)),
			,
			Организация.Код &lt;&gt; "000000005"
				И Субконто2 В ИЕРАРХИИ
					(ВЫБРАТЬ
						Таблица.Ссылка
					ИЗ
						Справочник.СтатьиДоходовРасходов КАК Таблица
					ГДЕ
						Таблица.Код = "6    "),
			НЕ КорСчет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Продажи), ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Инвестиции), ЗНАЧЕНИЕ(ПланСчетов.Финансовый.ПрибыльПрошлыхМесяцев), ЗНАЧЕНИЕ(ПланСчетов.Финансовый.ПрибыльТекущегоПериода))
				И НЕ КорСчет В ИЕРАРХИИ
						(ВЫБРАТЬ
							Таблица.Ссылка
						ИЗ
							ПланСчетов.Финансовый КАК Таблица
						ГДЕ
							Таблица.Код = "02"),
			) КАК Таблица
ГДЕ
	Таблица.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	0,
	Таблица.Период,
	МЕСЯЦ(Таблица.Период),
	ГОД(Таблица.Период),
	Таблица.Счет,
	Таблица.Субконто2,
	Таблица.Субконто1,
	ВЫРАЗИТЬ(Таблица.Субконто1 КАК Справочник.СтруктурныеЕдиницы).id_TT,
	Таблица.Субконто2,
	ВЫРАЗИТЬ(Таблица.Субконто2 КАК Справочник.СтатьиДоходовРасходов).Код,
	Таблица.СуммаОборот,
	Таблица.КорСчет
ИЗ
	РегистрБухгалтерии.Финансовый.Обороты(
			ДАТАВРЕМЯ(2017, 5, 1) {(&amp;НП)},
			&amp;КонецПериода,
			Месяц,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Продажи)),
			,
			Организация.Код &lt;&gt; "000000005"
				И Субконто2 В ИЕРАРХИИ
					(ВЫБРАТЬ
						Таблица.Ссылка
					ИЗ
						Справочник.СтатьиДоходовРасходов КАК Таблица
					ГДЕ
						Таблица.Код = "6    "),
			НЕ КорСчет.Код = "44.5"
				И НЕ КорСчет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Инвестиции), ЗНАЧЕНИЕ(ПланСчетов.Финансовый.ПрибыльПрошлыхМесяцев), ЗНАЧЕНИЕ(ПланСчетов.Финансовый.ПрибыльТекущегоПериода))
				И НЕ КорСчет В ИЕРАРХИИ
						(ВЫБРАТЬ
							Таблица.Ссылка
						ИЗ
							ПланСчетов.Финансовый КАК Таблица
						ГДЕ
							Таблица.Код = "02"),
			) КАК Таблица
ГДЕ
	Таблица.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
;

