////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СведенияОСтопОплатеСрезПоследних.Контрагент,
	СведенияОСтопОплатеСрезПоследних.СтопОплата
ПОМЕСТИТЬ ВТ_СтопОплата
ИЗ
	РегистрСведений.СведенияОСтопОплате.СрезПоследних(&amp;ТекущаяДата, ) КАК СведенияОСтопОплатеСрезПоследних
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОНЕЦПЕРИОДА(РегламентированныйПроизводственныйКалендарь.ДатаКалендаря, МЕСЯЦ) КАК Месяц
ПОМЕСТИТЬ ВТ_Период
ИЗ
	РегистрСведений.РегламентированныйПроизводственныйКалендарь КАК РегламентированныйПроизводственныйКалендарь
ГДЕ
	РегламентированныйПроизводственныйКалендарь.ДатаКалендаря МЕЖДУ &amp;ДатаНач И &amp;ДатаКон

СГРУППИРОВАТЬ ПО
	КОНЕЦПЕРИОДА(РегламентированныйПроизводственныйКалендарь.ДатаКалендаря, МЕСЯЦ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДоговорыКонтрагентов.Владелец,
	МАКСИМУМ(ДоговорыКонтрагентов.Дата) КАК Дата
ПОМЕСТИТЬ ВТ_Договоры
ИЗ
	Справочник.ДоговорыКонтрагентов КАК ДоговорыКонтрагентов
ГДЕ
	ДоговорыКонтрагентов.ПометкаУдаления = ЛОЖЬ

СГРУППИРОВАТЬ ПО
	ДоговорыКонтрагентов.Владелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ФинансовыйОбороты.Субконто1 КАК Ссылка
ПОМЕСТИТЬ ВТ_Контрагенты
ИЗ
	РегистрБухгалтерии.Финансовый.Обороты(
			,
			,
			,
			,
			ЗНАЧЕНИЕ(ПланВидовхарактеристик.ВидыСубконто.Контрагенты),
			Субконто1 В ИЕРАРХИИ (&amp;ГруппаКонтр)
				И НЕ Субконто1.РазовыйКонтрагент {(Субконто1.ОсновнойБухгалтерПокупателя).*, (Субконто1.Ссылка).*},
			,
			) КАК ФинансовыйОбороты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Контрагенты.Ссылка,
	Контрагенты.ЭДО,
	Контрагенты.КомментарийЭДОДата,
	Контрагенты.ДатаПодключенияКЭДО,
	ВТ_Период.Месяц КАК Месяц,
	1 КАК ВсегоКонтрагентов,
	ВЫБОР
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И Контрагенты.ДатаПодключенияКЭДО = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 1
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И КОНЕЦПЕРИОДА(Контрагенты.ДатаПодключенияКЭДО, МЕСЯЦ) &lt;= ВТ_Период.Месяц
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ПодключеноКЭДО,
	ВЫБОР
		КОГДА Контрагенты.ЭДО = ЛОЖЬ
			ТОГДА 1
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И КОНЕЦПЕРИОДА(Контрагенты.ДатаПодключенияКЭДО, МЕСЯЦ) &gt; ВТ_Период.Месяц
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК НеПодключеноКЭДО,
	ВЫБОР
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И КОНЕЦПЕРИОДА(Контрагенты.ДатаПодключенияКЭДО, МЕСЯЦ) = ВТ_Период.Месяц
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ПодключеноКЭДОВЭТОММесяце,
	ВЫБОР
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И КОНЕЦПЕРИОДА(Контрагенты.ДатаПодключенияКЭДО, МЕСЯЦ) = ВТ_Период.Месяц
			ТОГДА "Переведен в отчетном мес."
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И Контрагенты.ДатаПодключенияКЭДО = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА "Переведен"
		КОГДА Контрагенты.ЭДО = ИСТИНА
				И КОНЕЦПЕРИОДА(Контрагенты.ДатаПодключенияКЭДО, МЕСЯЦ) &lt; ВТ_Период.Месяц
			ТОГДА "Переведен"
		ИНАЧЕ "Не переведен"
	КОНЕЦ КАК Текст,
	Контрагенты.КомментарийЭДО,
	ВТ_СтопОплата.СтопОплата,
	ВТ_ПрикрепленныеФайлы.ЭДО КАК ЕстьПрикрепленныйФайл,
	ВТ_Договоры.Дата КАК ДатаЗаключенияДоговора
ИЗ
	ВТ_Период КАК ВТ_Период,
	Справочник.Контрагенты КАК Контрагенты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СтопОплата КАК ВТ_СтопОплата
		ПО Контрагенты.Ссылка = ВТ_СтопОплата.Контрагент
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПрикрепленныеФайлы КАК ВТ_ПрикрепленныеФайлы
		ПО Контрагенты.Ссылка = ВТ_ПрикрепленныеФайлы.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Договоры КАК ВТ_Договоры
		ПО Контрагенты.Ссылка = ВТ_Договоры.Владелец
ГДЕ
	Контрагенты.Ссылка В
			(ВЫБРАТЬ
				ВТ_Контрагенты.Ссылка
			ИЗ
				ВТ_Контрагенты)
	И ЕСТЬNULL(ВТ_СтопОплата.СтопОплата, ЛОЖЬ) = ЛОЖЬ
	И ЕСТЬNULL(ВТ_Договоры.Дата, ДАТАВРЕМЯ(1, 1, 1)) &lt;= ВТ_Период.Месяц</query>
	</dataSet>
	<calculatedField>
		<dataPath>Поле1</dataPath>
		<expression>"Общее количество контрагентов"</expression>
	</calculatedField>
