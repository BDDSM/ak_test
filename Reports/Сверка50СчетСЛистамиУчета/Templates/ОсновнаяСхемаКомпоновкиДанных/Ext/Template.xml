			<field>Организация</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОстатокНа50Счету</dataPath>
			<field>ОстатокНа50Счету</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Остаток на 50 счете</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОстатокПоЛистуУчета</dataPath>
			<field>ОстатокПоЛистуУчета</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтруктурнаяЕдиница</dataPath>
			<field>СтруктурнаяЕдиница</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.СтруктурныеЕдиницы</v8:Type>
			</valueType>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЕСТЬNULL(ФинансовыйОстатки.Субконто1, ЛистУчета.ТорговаяТочка) КАК СтруктурнаяЕдиница,
	ЕСТЬNULL(ФинансовыйОстатки.СуммаОстаток, 0) КАК ОстатокНа50Счету,
	ЕСТЬNULL(ЛистУчета.КонечныйОстаток, 0) КАК ОстатокПоЛистуУчета
ИЗ
	РегистрБухгалтерии.Финансовый.Остатки({(ДОБАВИТЬКДАТЕ(КОНЕЦПЕРИОДА(&amp;ДатаОтчета, ДЕНЬ), СЕКУНДА, 1))}, Счет = ЗНАЧЕНИЕ(ПланСчетов.Финансовый.Касса), , {(Организация) КАК Организация}) КАК ФинансовыйОстатки
		ПОЛНОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ЛистУчета.ТорговаяТочка КАК ТорговаяТочка,
			ЛистУчета.КонечныйОстаток КАК КонечныйОстаток
		ИЗ
			Документ.ЛистУчета КАК ЛистУчета
		ГДЕ
			ЛистУчета.Дата МЕЖДУ НАЧАЛОПЕРИОДА(&amp;ДатаОтчета, ДЕНЬ) И КОНЕЦПЕРИОДА(&amp;ДатаОтчета, ДЕНЬ)
		{ГДЕ
			ЛистУчета.Организация.* КАК Организация}) КАК ЛистУчета
		ПО ФинансовыйОстатки.Субконто1 = ЛистУчета.ТорговаяТочка</query>
	</dataSet>
	<totalField>
		<dataPath>ОстатокНа50Счету</dataPath>
		<expression>Сумма(ОстатокНа50Счету)</expression>
	</totalField>
	<totalField>
		<dataPath>ОстатокПоЛистуУчета</dataPath>
		<expression>Сумма(ОстатокПоЛистуУчета)</expression>
	</totalField>
