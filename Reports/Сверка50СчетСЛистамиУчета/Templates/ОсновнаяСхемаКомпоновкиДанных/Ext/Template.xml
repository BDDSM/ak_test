						<dcsset:item>
							<dcsset:field>ОстатокПоЛистуУчета</dcsset:field>
						</dcsset:item>
					</dcsset:selection>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
