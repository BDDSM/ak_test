
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)
	Если ТипЗнч(ПараметрКоманды)<>Тип("СправочникСсылка.CMS1C_НастройкиСайтов")
		И ТипЗнч(ПараметрКоманды) <> ТИп("СправочникСсылка.CMS1C_НастройкиОбменаССайтом")
		Тогда
	
		//Список = CMS1C_ПроцедурыОбменаССайтомСерверСВызовом.ПолучитьСписокНастроекОбмена();
		//Настройка = Список.ВыбратьЭлемент("Выберите сайт для получения заказов..."); 
		//Если Настройка=Неопределено Тогда	 
		//	возврат   	 
		//КонецЕсли; 
		//ПараметрКоманды = Настройка.Значение;
		CMS1C_ПроцедурыОбменаССайтом.ВыполнитьЗагрузкуЗаказов();
	КонецЕсли; 
	
	Параметры = Новый Структура;
	
	СписокНастроек = Новый СписокЗначений();
	Если ТипЗнч(ПараметрКоманды) = Тип("СправочникСсылка.CMS1C_НастройкиСайтов") Тогда
		СписокНастроек.Добавить(ПараметрКоманды);
	ИначеЕсли ТипЗнч(ПараметрКоманды) = Тип("СправочникСсылка.CMS1C_НастройкиОбменаССайтом") Тогда
		СписокНастроек.Добавить(ПараметрКоманды.Владелец);
	КонецЕсли;
	
	Состояние("Получение данных о заказах на сайте "+СписокНастроек[0].Значение+"...");
	
	
	Параметры.Вставить("СписокНастроекСайтов",СписокНастроек);
	ОткрытьФорму("Справочник.CMS1C_НастройкиСайтов.Форма.ФормаЗагрузкиЗаказов",Параметры);
КонецПроцедуры
