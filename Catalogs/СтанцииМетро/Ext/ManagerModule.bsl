
Процедура ЗагрузитьИзКлассификатора() Экспорт
	
	Макет = Справочники.СтанцииМетро.ПолучитьМакет("Станции");
	Для н = 1 По Макет.ВысотаТаблицы Цикл
		Область = Макет.Область(н, 1, н, 1);
		Ссылка = Справочники.СтанцииМетро.НайтиПоНаименованию(СокрЛП(Область.Текст));
		Если Ссылка.Пустая() Тогда
			СпрОбъект = Справочники.СтанцииМетро.СоздатьЭлемент();
			СпрОбъект.Наименование = СокрЛП(Область.Текст);
			СпрОбъект.Записать();
		КонецЕсли;	
	КонецЦикла;	
	
КонецПроцедуры	