
Процедура ПриОткрытии()
	Отб=ХранилищеОбщихНастроек.Загрузить("АКfield_operation");
	Если Отб<>Неопределено Тогда
		СправочникСписок.Отбор.table_operation.Установить("td_move");
		СправочникСписок.Отбор.field_operation.Установить(Отб);
	КонецЕсли; 
	//ХранилищеОбщихНастроек.Сохранить("АК_ТабДанныеИзб",,ТабДанные);
КонецПроцедуры
