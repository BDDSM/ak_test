				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументРеализацияТоваровУслуг.Ссылка,
	ДокументРеализацияТоваровУслуг.Номер,
	ДокументРеализацияТоваровУслуг.Дата,
	ДокументРеализацияТоваровУслуг.Контрагент,
	ДокументРеализацияТоваровУслуг.Организация,
	ДокументРеализацияТоваровУслуг.СуммаДокумента,
	ДокументРеализацияТоваровУслуг.Комментарий,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ЭДО_ИсходящийДокумент.Ссылка) КАК ДокументЭДО
ИЗ
	Документ.РеализацияТоваровУслуг КАК ДокументРеализацияТоваровУслуг
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЭДО_ИсходящийДокумент КАК ЭДО_ИсходящийДокумент
		ПО ДокументРеализацияТоваровУслуг.Ссылка = ЭДО_ИсходящийДокумент.ДокументОснование
			И (ЭДО_ИсходящийДокумент.ПометкаУдаления = ЛОЖЬ)

СГРУППИРОВАТЬ ПО
	ДокументРеализацияТоваровУслуг.Ссылка,
	ДокументРеализацияТоваровУслуг.Номер,
	ДокументРеализацияТоваровУслуг.Дата,
	ДокументРеализацияТоваровУслуг.Контрагент,
	ДокументРеализацияТоваровУслуг.Организация,
	ДокументРеализацияТоваровУслуг.СуммаДокумента,
	ДокументРеализацияТоваровУслуг.Комментарий</QueryText>
				<MainTable>Document.РеализацияТоваровУслуг</MainTable>
